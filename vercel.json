{
    "rewrites": [
        {
            "source": "/openai-api/:path*",
            "destination": "/api/proxy-openai?path=/:path*"
        },
        {
            "source": "/api/:path*",
            "destination": "/api/proxy-legacy?path=/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}